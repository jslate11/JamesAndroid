<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.james.jamesandroid.MainActivity">

    <TextView
        android:text="Carrier No."
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:id="@+id/textView"
        app:layout_constraintBaseline_toBaselineOf="@+id/editText"
        android:layout_marginStart="32dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="32dp" />

    <TextView
        android:text="Model Type"
        android:layout_height="wrap_content"
        android:id="@+id/textView2"
        android:layout_width="75dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/editText2"
        android:layout_marginStart="32dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="32dp" />

    <EditText
        android:layout_height="40dp"
        android:inputType="numberDecimal"
        android:ems="10"
        android:id="@+id/editText3"
        android:layout_marginTop="32dp"
        app:layout_constraintTop_toBottomOf="@+id/editText2"
        android:layout_marginStart="8dp"
        app:layout_constraintLeft_toRightOf="@+id/textView3"
        android:layout_marginLeft="8dp"
        android:layout_width="80dp" />

    <TextView
        android:text="VIN No."
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:id="@+id/textView3"
        tools:layout_editor_absoluteX="32dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/editText3" />

    <EditText
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:inputType="numberDecimal"
        android:ems="10"
        android:id="@+id/editText4"
        android:layout_marginTop="32dp"
        app:layout_constraintTop_toBottomOf="@+id/editText3"
        android:layout_marginStart="8dp"
        app:layout_constraintLeft_toRightOf="@+id/textView4"
        android:layout_marginLeft="8dp" />

    <TextView
        android:text="Line 1"
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:id="@+id/textView4"
        app:layout_constraintBaseline_toBaselineOf="@+id/editText4"
        android:layout_marginStart="32dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="32dp" />

    <TextView
        android:text="Line 2"
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:id="@+id/textView5"
        android:layout_marginStart="32dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="32dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/editText5" />

    <EditText
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:inputType="numberDecimal"
        android:ems="10"
        android:id="@+id/editText"
        android:layout_marginTop="32dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginStart="8dp"
        app:layout_constraintLeft_toRightOf="@+id/textView"
        android:layout_marginLeft="8dp" />

    <EditText
        android:layout_height="40dp"
        android:inputType="numberDecimal"
        android:ems="10"
        android:id="@+id/editText5"
        android:layout_width="40dp"
        android:layout_marginTop="32dp"
        app:layout_constraintTop_toBottomOf="@+id/editText4"
        android:layout_marginStart="8dp"
        app:layout_constraintLeft_toRightOf="@+id/textView5"
        android:layout_marginLeft="8dp" />

    <EditText
        android:layout_height="40dp"
        android:inputType="numberDecimal"
        android:ems="10"
        android:id="@+id/editText7"
        android:layout_width="40dp"
        android:layout_marginEnd="32dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="32dp"
        android:layout_marginTop="32dp"
        app:layout_constraintTop_toBottomOf="@+id/editText6" />

    <EditText
        android:layout_height="40dp"
        android:inputType="numberDecimal"
        android:ems="10"
        android:id="@+id/editText8"
        android:layout_width="40dp"
        android:layout_marginTop="32dp"
        app:layout_constraintTop_toBottomOf="@+id/editText7"
        android:layout_marginEnd="32dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="32dp" />

    <EditText
        android:layout_height="40dp"
        android:inputType="numberDecimal"
        android:ems="10"
        android:id="@+id/editText9"
        android:layout_width="40dp"
        android:layout_marginTop="32dp"
        app:layout_constraintTop_toBottomOf="@+id/editText8"
        android:layout_marginEnd="32dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="32dp" />

    <EditText
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:inputType="numberDecimal"
        android:ems="10"
        android:id="@+id/editText6"
        android:layout_marginTop="32dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginEnd="32dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="32dp" />

    <EditText
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:inputType="numberDecimal"
        android:ems="10"
        android:id="@+id/editText2"
        android:layout_marginTop="32dp"
        app:layout_constraintTop_toBottomOf="@+id/editText"
        android:layout_marginStart="8dp"
        app:layout_constraintLeft_toRightOf="@+id/textView2"
        android:layout_marginLeft="8dp" />

    <RadioGroup
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:checkedButton="@+id/radioButtonRead"
        tools:layout_editor_absoluteY="408dp"
        android:layout_width="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="16dp">

        <RadioButton
            android:text="Read"
            android:layout_height="wrap_content"
            android:id="@+id/radioButtonRead"
            android:layout_weight="1"
            android:textColorLink="@android:color/holo_green_light"
            android:layout_width="200dp"
            android:textAlignment="viewStart" />

        <RadioButton
            android:text="Write"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/radioButtonWrite"
            android:layout_weight="1"
            />
    </RadioGroup>

    <TextView
        android:text="Line 3"
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:id="@+id/textView6"
        app:layout_constraintRight_toLeftOf="@+id/editText6"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/editText6" />

    <TextView
        android:text="Line 5"
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:id="@+id/textView8"
        app:layout_constraintRight_toLeftOf="@+id/editText8"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/editText8" />

    <TextView
        android:text="Seq No."
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:id="@+id/textView9"
        app:layout_constraintRight_toLeftOf="@+id/editText9"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/editText9" />

    <TextView
        android:text="Line 4"
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:id="@+id/textView7"
        app:layout_constraintRight_toLeftOf="@+id/editText7"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/editText7" />

</android.support.constraint.ConstraintLayout>
